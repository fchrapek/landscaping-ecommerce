    "babel-plugin-styled-components": "^1.12.0",
    "date-fns": "^2.16.1",
    "downshift": "^6.0.6",
    "graphql": "16.0.1",
    "graphql-client": "^2.0.1",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
